<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.effect.InnerShadow?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="524.0" prefWidth="820.0" style="-fx-background-color: #73bbff;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="yaksa.chemist.controller.chart.ContYKChart">
   <children>
      <Pane prefHeight="100.0" prefWidth="696.0">
         <children>
            <HBox alignment="CENTER" layoutY="12.0" prefHeight="79.0" prefWidth="820.0" style="-fx-background-color: #f0f0f0;">
               <children>
                  <Label fx:id="ChmiName" text="약국이름" textFill="#363636">
                     <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </HBox.margin>
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Label>
                  <Label text="님" textFill="#363636">
                     <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </HBox.margin>
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Label>
                  <ComboBox fx:id="ChartCB" prefWidth="150.0">
                     <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </HBox.margin>
                  </ComboBox>
                  <Button fx:id="OkBtn" mnemonicParsing="false" onAction="#LookChart" styleClass="btnManage" stylesheets="@../../css/main/fullpackstyling.css" text="확인">
                     <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </HBox.margin>
                     <effect>
                        <InnerShadow color="WHITE" />
                     </effect>
                     <font>
                        <Font size="13.0" />
                     </font>
                  </Button>
               </children>
            </HBox>
         </children>
      </Pane>
      <VBox fx:id="ChartVBox" alignment="CENTER" prefHeight="350.0" prefWidth="100.0">
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </VBox>
   </children>
</VBox>
